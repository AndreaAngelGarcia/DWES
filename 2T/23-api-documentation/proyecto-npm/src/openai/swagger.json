{
    "openapi": "3.0.3",
    "info": {
        "version": "0.0.1",
        "title": "My editor notes API",
        "description": "RESTfull API for editor notes in memory",
        "termsOfService": "http://swagger.io/terms/"
    },
    "servers": [{
        "url":"http://localhost:3000/api/v1"
    }],
    "paths": {
        "/": {
            "get": {
                "tags": [
                    "Notas"
                ],
                "summary": "Recoger todas las notas en memoria",
                "description": "Recoger las notas que est√°n almacenadas en memoria",
                "parameters": [
                    {}
                ],
                "responses": {
                    "200": {
                        "description": "Lista de notas",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "format": "int64"
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                    
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {}
}
